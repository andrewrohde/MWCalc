<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.madwindevelopers.mwcalc">

     <uses-sdk android:minSdkVersion="16" android:targetSdkVersion="18"/>

    <uses-permission android:name="android.permission.INTERNET"/>

     <application android:icon="@drawable/mwcalc_icon" android:label="@string/app_name">
            <activity android:label="@string/app_name" android:name="com.madwindevelopers.mwcalc.InterfaceUI" android:screenOrientation="sensor" android:theme="@android:style/Theme.NoTitleBar" android:windowSoftInputMode="stateAlwaysHidden">
                <intent-filter>
                    <action android:name="android.intent.action.MAIN"/>

                    <category android:name="android.intent.category.LAUNCHER"/>
                </intent-filter>
            </activity>
        <activity android:label="@string/settings" android:name="com.madwindevelopers.mwcalc.PreferencesUI" android:windowSoftInputMode="stateAlwaysHidden"/>
         <activity android:label="About" android:name="com.madwindevelopers.mwcalc.HelpUI" android:windowSoftInputMode="stateAlwaysHidden"/>
         <activity android:label="Unit Converter" android:name="com.madwindevelopers.mwcalc.UnitConverter"/>

     </application>

</manifest>
